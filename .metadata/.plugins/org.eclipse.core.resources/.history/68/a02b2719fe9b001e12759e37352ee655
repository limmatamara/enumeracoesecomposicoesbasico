package entities;

import entities.enums.WorkerLevel;

public class Worker {
	public String name;
	public WorkerLevel level;
	public Double baseSalary;
	
	public Worker() {
		
	}

	public Worker(String name, WorkerLevel level, Double baseSalary) {
		this.name = name;
		this.level = level;
		this.baseSalary = baseSalary;
	}
	
	
}
